org 0

if 0
	nop ;= FF
endif

if 1
	nop ;= 00
else
	nop ;= FF
endif

if 1 == 2
	nop ;= FF
elseif 2 == 3
	nop ;= FF 
else
	nop ;= 00
endif


macro repeating_macro(n)
	if <n> >= 1
		%repeating_macro(<n> - 1)
		mov a, <n>
	else
	endif
endmacro

start_repeat:
	%repeating_macro(7)
end_repeat:
	db (end_repeat - start_repeat) ;= 0E
